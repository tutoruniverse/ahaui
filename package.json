{
  "name": "ahaui",
  "version": "2.1.6",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=./packages/react --filter=./packages/css --parallel",
    "build:tokens": "turbo run build  --filter=./packages/ahaui-design-tokens",
    "build:css": "turbo run build  --filter=./packages/css",
    "build:icons": "turbo run build  --filter=./packages/icons",
    "build:react": "turbo run build  --filter=./packages/react",
    "build:core": "turbo run build  --filter=./packages/core",
    "build:docs": "turbo run build --filter=./apps/docs",
    "dev": "turbo run dev --parallel",
    "dev:docs": "turbo run dev --filter=./apps/docs",
    "dev:css": "turbo run dev  --filter=./packages/css",
    "dev:icons": "turbo run dev  --filter=./packages/icons",
    "dev:react": "turbo run dev  --filter=./packages/react",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "version": "turbo changeset version"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-external-helpers": "^7.17.12",
    "@babel/plugin-syntax-class-properties": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-optional-chaining": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.1.0",
    "babel-runtime": "^6.26.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "4.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "^2.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fs-extra": "7.0.1",
    "husky": ">=4",
    "lint-staged": "10.5.2",
    "prettier": "latest",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.75.3",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "turbo": "latest",
    "typescript": "^3.8.0",
    "yargs": "^16.2.0",
    "vite": "^2.9.1",
    "webpack": "^4.43.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/ahaui#readme",
  "slack": "https://ahaui.slack.com/join/shared_invite/zt-18rg4yd00-B6ob9D2DgXXVILSWh6CNrQ#/shared-invite/email",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  },
  "packageManager": "yarn@1.22.18"
}
