{
  "name": "@ahaui/react",
  "version": "3.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es/*.{js,map}",
    "lib/*.{js,map}",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "yarn build",
    "dev": "rollup -c -w",
    "format": "eslint --ext js src --fix",
    "lint": "eslint --ext js src"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@restart/hooks": "0.3.25",
    "@types/react-tagsinput": "3.19.9",
    "classnames": "2.2.6",
    "dom-helpers": "5.1.0",
    "popper.js": "1.15.0",
    "prop-types": "15.7.2",
    "prop-types-extra": "1.1.0",
    "rc-slider": "8.7.1",
    "react-slick": "0.27.13",
    "react-split-pane": "0.1.92",
    "react-tagsinput": "3.19.0",
    "react-textarea-autosize": "7.1.2",
    "react-toastify": "7.0.4",
    "react-transition-group": "4.0.0",
    "uncontrollable": "7.1.1",
    "warning": "4.0.3",
    "lodash.once": "4.1.1"
  },
  "peerDependencies": {
    "react": "^16.9.0 || 17.x",
    "react-dom": "^16.9.0 || 17.x"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/ahaui/blob/main/packages/react#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  },
  "description": "Aha Design System - React",
  "directories": {
    "lib": "lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
